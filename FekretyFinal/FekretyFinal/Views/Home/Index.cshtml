@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0" />
    <title>MU Material : Home</title>

    <!-- CSS  -->
    <link href="~/Content/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <!-- Font Awesome -->
    <link href="~/Content/css/font-awesome.css" rel="stylesheet">
    <!-- Skill Progress Bar -->
    <link href="~/Content/css/pro-bars.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Content/css/owl.carousel.css">
    <!-- Default Theme CSS File-->
    <link id="switcher" href="~/Content/css/themes/default-theme.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <!-- Main css File -->
    <link href="~/Content/style.css" type="text/css" rel="stylesheet" media="screen,projection" />

    <!-- Font -->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>

<body>
  
    <header id="header" role="banner">
        <div class="navbar-fixed">
            <nav>
                <div class="container">
                    <div class="nav-wrapper">

                        <!-- LOGO -->
                        <!-- TEXT BASED LOGO -->
                        <a class="brand-logo">LOGO</a>

                        <!-- Image Based Logo -->
                        <!-- <a href="index.html" class="brand-logo"><img src="img/logo.jpeg" alt="logo img"></a>  -->
                        <ul class="right hide-on-med-and-down custom-nav menu-scroll">
                            <li class="active"><a href=@Url.Action("Index", "Home")>Home</a></li>
                            <li><a href =@Url.Action("ITEMS", "Home")>ITEMS</a></li>
                            <li><a href =@Url.Action("REGISTER", "Home")>REGISTER</a></li>                                          
                        </ul>
                        <!-- For Mobile View -->
                      <ul id="slide-out" class="side-nav menu-scroll">
                          <li class="active"><a href=@Url.Action("Index", "Home")>Home</a></li>
                            <li><a href=@Url.Action("ITEMS", "Home")>ITEMS</a></li>
                            <li><a href=@Url.Action("REGISTER", "Home")>REGISTER</a></li>
                      </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <div class="main-wrapper">
        <main role="main">
            <!-- START HOME SECTION -->
            <section id="home">
                <div class="overlay-section"  >
                    <div class="container">
                        <div class="row">
                            <div class="col s12">
                                <div class="home-inner">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- START ABOUT SECTION -->
            <section id="about">
                <div class="container">
                    <div class="row">
                        <div class="col s12">
                            <div class="about-inner">
                                <div class="row">
                                    <div class="col s12 m4 l3">
                                        <div class="about-inner-left">
                                            <p></p>
                                            <p></p>
                                            @using (Html.BeginForm("GetTextOfTextbox", "Home", FormMethod.Post))
                                            {
                                               <label for="exampleInputEmail1">Person Name</label>
                                               <input type="email" class="form-control" id="Personname" name="Personname" aria-describedby="emailHelp" placeholder="Name">
                                               <button type="submit" class="submitbutton">Submit</button>
                                             
                                            }
                                            @Html.ActionLink("Edit", "EditJsonPlaceHolderData")
                                            <br />
                                            @Html.ActionLink("Delete", "DeleteJsonPlaceHolderData")
                                        </div>
                                    </div>
                                    <div class="col s12 m8 l9">
                                        <div class="about-inner-right">                                         
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
 
      
            <!-- Start Footer -->
            <footer id="footer" role="contentinfo">
                <!-- Start Footer Top -->
                <div class="footer-top">
                    <div class="container">
                        <div class="row">
                            <div class="col s12">
                                <div class="footer-top-inner">
                                    <h2 class="title">Contact</h2>
                                    <p>In this website there is a Todo List  and you can submit feedback and update or delete in any element</p>
                                    <div class="contact">
                                        <div class="row">
                                            <div class="col s12 m6 l6">
                                                <div class="contact-form">
                                                    <form>
                                                        <p>Address: USA </p>
                                                        <p>Email: contact@gmail.com </p>
                                                        <p>Phone: 9542015200555514455 </p>
                                                    </form>
                                                </div>
                                            </div>
                                            <div class="col s12 m6 l6">
                                                <div class="contact-map">
                                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3281.297314036103!2d-86.74954699999999!3d34.672444999999996!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88626565a94cdb25%3A0x74c206053b6a97c9!2s305+Intergraph+Way%2C+Madison%2C+AL+35758%2C+USA!5e0!3m2!1sen!2sbd!4v1431591462160" width="100%" height="100%" frameborder="0" style="border:0"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Start Footer Bottom -->
                <div class="footer-bottom">
                    <div class="container">
                        <div class="row">
                            <div class="col s12">
                                <div class="footer-inner">
                                    <!-- Bottom to Up Btn -->
                                    <p class="design-info">Designed By <a href="https://www.google.com/?hl=ar">Google</a></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </main>
    </div>
    <!-- jQuery Library -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <!-- Materialize js -->
    <script type="text/javascript" src="js/materialize.min.js"></script>
    <!-- Skill Progress Bar -->
    <script src="~/Content/js/appear.min.js" type="text/javascript"></script>
    <script src="~/Content/js/pro-bars.min.js" type="text/javascript"></script>
    <!-- Owl slider -->
    <script src="~/Content/js/owl.carousel.min.js"></script>
    <!-- Mixit slider  -->
    <script src="http://cdn.jsdelivr.net/jquery.mixitup/latest/jquery.mixitup.min.js"></script>
    <!-- counter -->
    <script src="~/Content/js/waypoints.min.js"></script>
    <script src="~/Content/js/jquery.counterup.min.js"></script>

    <!-- Custom Js -->
    <script src="~/Content/js/custom.js"></script>
    <script>
        $(document).ready(function () {
            $(".submitbutton").attr('disabled', 'disabled');

            $("#Personname").keyup(function () {
                $(".submitbutton").removeAttr('disabled');
            });

            $(".submitbutton").click(function () {
                $(this).attr('disabled', 'disabled');
            });
        });
    </script>
</body>
</html>